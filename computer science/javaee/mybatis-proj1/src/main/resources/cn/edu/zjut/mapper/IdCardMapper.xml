<!DOCTYPE mapper
        PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.zjut.mapper.IdCardMapper">
    <select id="findById" parameterType="integer" resultType="cn.edu.zjut.po.IdCard">
        select * from id_card where id = #{id}
